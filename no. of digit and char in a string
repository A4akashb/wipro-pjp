using System;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter a string");
            string str = Console.ReadLine();
            int n=0, s=0,i;
            for (i = 0; i < str.Length; i++)
            {
                int temp = str[i];
                if (temp>47 && temp<58)
                {
                    n++;
                }
                else
                {
                    if((temp>64 && temp<91)||(temp>96 && temp < 123)){
                        s++;
                    }
                }
            }
            Console.WriteLine(n);
        }
    }
}
