using System;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter the string");
            string str = Console.ReadLine();
            
            string new1="";

            int i;
            for (i = 0; i < str.Length; i++)
            {
                char temp = str[i];
                temp++;
                new1 = new1 + temp;
                
            }
            Console.WriteLine(new1);
        }
    }
}
